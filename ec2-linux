terraform {
  required_providers {
    aws = {
      source = "hashicorp/aws"
      version = "6.16.0"
    }
  }
}

provider "aws" {
  region = "us-east-1"
}

resource "aws_instance" "webserver" {
  ami           = "ami-052064a798f08f0d3"
  instance_type = "t3.micro"
  count         = "2"

  tags = {
    Name = "linux"
  }
}
